<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.rakesh.eventmanagement">
    <uses-permission android:name="android.permission.WAKE_LOCK" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".eventmanagement.AddEvent" />
        <activity android:name=".alarm.AlarmReceiverActivity" />
        <activity android:name=".eventmanagement.DeleteEvent" />
        <activity android:name=".HomeEventManagement" />
        <activity android:name=".eventmanagement.GetAllEvent" />
        <activity android:name=".eventmanagement.UpdateEvent"></activity>
    </application>

</manifest>
